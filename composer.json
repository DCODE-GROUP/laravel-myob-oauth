{
  "name": "dcodegroup/laravel-myob-oauth",
  "description": "Simple package which dcode uses to handle the connection to MYOB for all projects. Provides endpoints for UI and callbacks for MYOB",
  "keywords": [
    "laravel",
    "myob",
    "dcode",
    "oauth"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Nathan Rzepecki",
      "email": "nathan@lionslair.net.au",
      "homepage": "https://www.lionslair.net.au",
      "role": "Developer"
    },
    {
      "name": "Dcode Group",
      "email": "forge@dcodegroup.com",
      "homepage": "https://dcodegroup.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^7.0  | ^8.0",
    "league/oauth2-client": "1.*|2.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "Dcodegroup\\LaravelMyobOauth\\": "src"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dcodegroup\\LaravelMyobOauth\\LaravelMyobOauthServiceProvider"
      ]
    }
  }
}
